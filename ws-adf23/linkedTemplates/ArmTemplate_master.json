{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ws-adf23"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"ls_in_excel_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_in_excel'"
		},
		"ls_in_json_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_in_json'"
		},
		"ls_in_excel_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlbikes.dfs.core.windows.net/"
		},
		"ls_in_json_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlvancouver.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ws-adf23_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"ls_in_excel_accountKey": {
						"value": "[parameters('ls_in_excel_accountKey')]"
					},
					"ls_in_json_accountKey": {
						"value": "[parameters('ls_in_json_accountKey')]"
					},
					"ls_in_excel_properties_typeProperties_url": {
						"value": "[parameters('ls_in_excel_properties_typeProperties_url')]"
					},
					"ls_in_json_properties_typeProperties_url": {
						"value": "[parameters('ls_in_json_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}